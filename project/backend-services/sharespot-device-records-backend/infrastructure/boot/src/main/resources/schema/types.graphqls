type DeviceRecords {
    device : Device
    entries : [RecordEntry]
}

type RecordEntry {
    label: String
    content : String
    type: RecordType
}

type Device {
    id: String
    name: String
}

type DeviceId {
    id: String
}

enum RecordType {
    BASIC
    SENSOR_DATA
}

input DeviceIdInput {
    deviceId: String
}

input DeviceRecordsInput {
    deviceId : String
    entries : [RecordEntryInput]
}

input RecordEntryInput {
    label: String
    content : String
    type: RecordType
}
