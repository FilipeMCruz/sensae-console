type Subscription {
    locations(Authorization: String) : GPSSensorData
    location(devices: [String], Authorization: String) : GPSSensorData
    locationByContent(content: String, Authorization: String) : GPSSensorData
}

type Query {
    history(filters: GPSSensorDataQuery) : [GPSSensorDataHistory]
    latest: [GPSSensorData]
    latestByDevice(devices: [String]): [GPSSensorData]
}
