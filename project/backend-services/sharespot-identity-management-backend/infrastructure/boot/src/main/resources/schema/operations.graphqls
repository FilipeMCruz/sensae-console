type Query {
    authenticate: AccessToken
    viewDomain(top: ViewDomain): [Domain]
    viewDomainInfo(domain: ViewDomain): DomainInfo
}

type Mutation {
    createDomain(domain: CreateDomain): Domain
    addTenant(instructions: AddTenantToDomain): Tenant
    removeTenant(instructions: RemoveTenantFromDomain): Tenant
    addDevice(instructions: AddDeviceToDomain): Device
    removeDevice(instructions: RemoveDeviceFromDomain): Device
}
