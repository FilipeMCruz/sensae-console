type GPSSensorData {
    dataId : String
    device : Sensor
    reportedAt : String
    data : SensorDataDetails
}

type Sensor {
    id: String
    name: String
    records : [RecordEntry]
}

type RecordEntry {
    label: String
    content: String
}

type SensorDataDetails {
    gps : GPSSensorDetails
}

type GPSSensorDetails {
    longitude : Float
    latitude : Float
}
