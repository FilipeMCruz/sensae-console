type GPSSensorData {
    dataId : String
    device : Sensor
    reportedAt : String
    data : SensorDataDetails
}

type Sensor {
    id: String
    name: String
    records : [RecordEntry]
}

type RecordEntry {
    label: String
    content: String
}

type SensorDataDetails {
    gps : GPSSensorDetails
    status : StatusSensorDetails
}

type GPSSensorDetails {
    longitude : Float
    latitude : Float
}

type StatusSensorDetails {
    motion: String
}

input GPSSensorDataQuery {
    device: [String]
    startTime : String
    endTime : String
}

type GPSSensorDataHistory {
    deviceId: String
    deviceName: String
    startTime : String
    endTime : String
    distance : Float
    data : [GPSSensorDetails]
}
