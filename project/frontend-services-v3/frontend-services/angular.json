{
	"version": 1,
	"projects": {
		"sharespot-device-records-frontend": {
			"projectType": "application",
			"root": "apps/sharespot-device-records-frontend",
			"sourceRoot": "apps/sharespot-device-records-frontend/src",
			"prefix": "frontend-services",
			"architect": {
				"build": {
					"builder": "@nrwl/angular:webpack-browser",
					"outputs": [
						"{options.outputPath}"
					],
					"options": {
						"outputPath": "dist/apps/sharespot-device-records-frontend",
						"index": "apps/sharespot-device-records-frontend/src/index.html",
						"main": "apps/sharespot-device-records-frontend/src/main.ts",
						"polyfills": "apps/sharespot-device-records-frontend/src/polyfills.ts",
						"tsConfig": "apps/sharespot-device-records-frontend/tsconfig.app.json",
						"assets": [
							"apps/sharespot-device-records-frontend/src/favicon.ico",
							"apps/sharespot-device-records-frontend/src/assets"
						],
						"styles": [
							"apps/sharespot-device-records-frontend/src/styles.css"
						],
						"scripts": [],
						"customWebpackConfig": {
							"path": "apps/sharespot-device-records-frontend/webpack.config.js"
						},
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/sharespot-device-records-frontend/src/environments/environment.ts",
									"with": "apps/sharespot-device-records-frontend/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"customWebpackConfig": {
								"path": "apps/sharespot-device-records-frontend/webpack.prod.config.js"
							}
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@nrwl/angular:webpack-server",
					"configurations": {
						"production": {
							"browserTarget": "sharespot-device-records-frontend:build:production"
						},
						"development": {
							"browserTarget": "sharespot-device-records-frontend:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 7084
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "sharespot-device-records-frontend:build",
						"extraWebpackConfig": "apps/sharespot-device-records-frontend/webpack.config.js"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/sharespot-device-records-frontend/src/**/*.ts",
							"apps/sharespot-device-records-frontend/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": [
						"coverage/apps/sharespot-device-records-frontend"
					],
					"options": {
						"jestConfig": "apps/sharespot-device-records-frontend/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"sharespot-device-records-frontend-e2e": {
			"root": "apps/sharespot-device-records-frontend-e2e",
			"sourceRoot": "apps/sharespot-device-records-frontend-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/sharespot-device-records-frontend-e2e/cypress.json",
						"devServerTarget": "sharespot-device-records-frontend:serve:development"
					},
					"configurations": {
						"production": {
							"devServerTarget": "sharespot-device-records-frontend:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": [
						"{options.outputFile}"
					],
					"options": {
						"lintFilePatterns": [
							"apps/sharespot-device-records-frontend-e2e/**/*.{js,ts}"
						]
					}
				}
			}
		},
