{
  "version": 2,
  "projects": {
    "sharespot-device-records-frontend": "apps/sharespot-device-records-frontend",
    "sharespot-device-records-frontend-e2e": "apps/sharespot-device-records-frontend-e2e",
    "sharespot-location-tracking-frontend": "apps/sharespot-location-tracking-frontend",
    "sharespot-location-tracking-frontend-e2e": "apps/sharespot-location-tracking-frontend-e2e",
    "ui-aggregator": "apps/ui-aggregator",
    "ui-aggregator-e2e": "apps/ui-aggregator-e2e"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "ui-aggregator"
}
