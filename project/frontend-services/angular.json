{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "yarn"
	},
	"newProjectRoot": "projects",
	"projects": {
		"ui-aggregator": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/ui-aggregator/project",
			"sourceRoot": "projects/ui-aggregator/project/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/ui-aggregator",
						"index": "projects/ui-aggregator/project/src/index.html",
						"main": "projects/ui-aggregator/project/src/main.ts",
						"polyfills": "projects/ui-aggregator/project/src/polyfills.ts",
						"tsConfig": "projects/ui-aggregator/project/tsconfig.app.json",
						"aot": true,
						"assets": [
							"projects/ui-aggregator/project/src/favicon.ico",
							"projects/ui-aggregator/project/src/assets"
						],
						"styles": [
							"projects/ui-aggregator/project/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/ui-aggregator/project/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "projects/ui-aggregator/project/src/environments/environment.ts",
									"with": "projects/ui-aggregator/project/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"extraWebpackConfig": "projects/ui-aggregator/project/webpack.prod.config.js"
						}
					}
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"options": {
						"browserTarget": "ui-aggregator:build",
						"port": 4200,
						"extraWebpackConfig": "projects/ui-aggregator/project/webpack.config.js"
					},
					"configurations": {
						"production": {
							"browserTarget": "ui-aggregator:build:production",
							"extraWebpackConfig": "projects/ui-aggregator/project/webpack.prod.config.js"
						}
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "ui-aggregator:build",
						"extraWebpackConfig": "projects/ui-aggregator/project/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/ui-aggregator/project/src/test.ts",
						"polyfills": "projects/ui-aggregator/project/src/polyfills.ts",
						"tsConfig": "projects/ui-aggregator/project/tsconfig.spec.json",
						"karmaConfig": "projects/ui-aggregator/project/karma.conf.js",
						"assets": [
							"projects/ui-aggregator/project/src/favicon.ico",
							"projects/ui-aggregator/project/src/assets"
						],
						"styles": [
							"projects/ui-aggregator/project/src/styles.css"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/ui-aggregator/project/tsconfig.app.json",
							"projects/ui-aggregator/project/tsconfig.spec.json",
							"projects/ui-aggregator/project/e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "projects/ui-aggregator/project/e2e/protractor.conf.js",
						"devServerTarget": "ui-aggregator:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "ui-aggregator:serve:production"
						}
					}
				}
			}
		}
	},
	"defaultProject": "ui-aggregator"
}