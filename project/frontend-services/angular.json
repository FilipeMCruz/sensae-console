{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"ui-aggregator": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "projects/ui-aggregator",
			"sourceRoot": "projects/ui-aggregator/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/ui-aggregator",
						"index": "projects/ui-aggregator/src/index.html",
						"main": "projects/ui-aggregator/src/main.ts",
						"polyfills": "projects/ui-aggregator/src/polyfills.ts",
						"tsConfig": "projects/ui-aggregator/tsconfig.app.json",
						"aot": true,
						"assets": [
							"projects/ui-aggregator/src/favicon.ico",
							"projects/ui-aggregator/src/assets"
						],
						"styles": [
							"projects/ui-aggregator/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/ui-aggregator/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "projects/ui-aggregator/src/environments/environment.ts",
									"with": "projects/ui-aggregator/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "3mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"extraWebpackConfig": "projects/ui-aggregator/webpack.prod.config.js"
						}
					}
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"options": {
						"browserTarget": "ui-aggregator:build",
						"port": 7000,
						"extraWebpackConfig": "projects/ui-aggregator/webpack.config.js"
					},
					"configurations": {
						"production": {
							"browserTarget": "ui-aggregator:build:production",
							"extraWebpackConfig": "projects/ui-aggregator/webpack.prod.config.js"
						}
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "ui-aggregator:build",
						"extraWebpackConfig": "projects/ui-aggregator/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/ui-aggregator/src/test.ts",
						"polyfills": "projects/ui-aggregator/src/polyfills.ts",
						"tsConfig": "projects/ui-aggregator/tsconfig.spec.json",
						"karmaConfig": "projects/ui-aggregator/karma.conf.js",
						"assets": [
							"projects/ui-aggregator/src/favicon.ico",
							"projects/ui-aggregator/src/assets"
						],
						"styles": [
							"node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
							"projects/ui-aggregator/src/styles.css"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/ui-aggregator/tsconfig.app.json",
							"projects/ui-aggregator/tsconfig.spec.json",
							"projects/ui-aggregator/e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "projects/ui-aggregator/e2e/protractor.conf.js",
						"devServerTarget": "ui-aggregator:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "ui-aggregator:serve:production"
						}
					}
				}
			}
		},
