{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2020",
    "module": "esnext",
    "lib": ["es2019", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@frontend-services/core": ["libs/core/src/index.ts"],
      "@frontend-services/data-processor/dto": [
        "libs/data-processor/dto/src/index.ts"
      ],
      "@frontend-services/data-processor/mapper": [
        "libs/data-processor/mapper/src/index.ts"
      ],
      "@frontend-services/data-processor/model": [
        "libs/data-processor/model/src/index.ts"
      ],
      "@frontend-services/data-processor/services": [
        "libs/data-processor/services/src/index.ts"
      ],
      "@frontend-services/device-records/dto": [
        "libs/device-records/dto/src/index.ts"
      ],
      "@frontend-services/device-records/mapper": [
        "libs/device-records/mapper/src/index.ts"
      ],
      "@frontend-services/device-records/model": [
        "libs/device-records/model/src/index.ts"
      ],
      "@frontend-services/device-records/services": [
        "libs/device-records/services/src/index.ts"
      ],
      "@frontend-services/fleet-management/dto": [
        "libs/fleet-management/dto/src/index.ts"
      ],
      "@frontend-services/fleet-management/mapper": [
        "libs/fleet-management/mapper/src/index.ts"
      ],
      "@frontend-services/fleet-management/model": [
        "libs/fleet-management/model/src/index.ts"
      ],
      "@frontend-services/fleet-management/services": [
        "libs/fleet-management/services/src/index.ts"
      ],
      "@frontend-services/identity-management/dto": [
        "libs/identity-management/dto/src/index.ts"
      ],
      "@frontend-services/identity-management/mapper": [
        "libs/identity-management/mapper/src/index.ts"
      ],
      "@frontend-services/identity-management/model": [
        "libs/identity-management/model/src/index.ts"
      ],
      "@frontend-services/identity-management/services": [
        "libs/identity-management/services/src/index.ts"
      ],
      "@frontend-services/mutual": ["libs/mutual/src/index.ts"],
      "@frontend-services/simple-auth-lib": [
        "libs/simple-auth-lib/src/index.ts"
